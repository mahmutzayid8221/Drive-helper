{
  "manifest_version": 3,
  "name": "Drive Folder Counts",
  "version": "1.1.0",
  "description": "Klasöre girmeden PDF/Video/Ses/Görsel sayacı",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://drive.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://drive.google.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}